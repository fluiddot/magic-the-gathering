    "react-native-elements": "^1.2.6",
    "react-native-web": "^0.11.7"
  },
  "devDependencies": {
    "babel-preset-expo": "^7.0.0",
    "jest": "^24.9.0",
    "jest-expo": "^25.1.0",
    "react-test-renderer": "16.8.3"
  },
  "private": true
